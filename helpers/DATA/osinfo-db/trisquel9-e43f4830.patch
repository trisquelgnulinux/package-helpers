+
+    <variant id="trisquel">
+      <name>Trisquel 9</name>
+    </variant>
+    <variant id="triskel">
+      <name>Triskel 9</name>
+    </variant>
+    <variant id="mini">
+      <name>Trisquel 9 Mini</name>
